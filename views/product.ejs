                                                        <div class="details">
                                                            <h1><%= c_data[0]['name'] %></h1>
                                                           

                                                            <div class="meta">
                                                                <div class="sku">
                                                               
                                                                    <span rel="tooltip" title="SKU is 0092"></span>
                                                                </div>

                                                                <div class="categories">
                                                                    <span><i class="icon-tags"></i><a href="/category?category=<%= c_data[0]['category'] %>" title="<%= c_data[0]['category'] %>"><%= c_data[0]['category'] %></a></span>
                                                                </div>
                                                            </div>
                                                        </div>

                                                        <div class="short-description">
														<span>Description</span>
