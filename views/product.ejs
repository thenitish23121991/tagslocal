                                                            <div class="row-fluid">
                                                                <div class="span6">
                                                                    
                                                                </div>
                                                            </div>
                                                        </div>

                                                       
                                                    </form>						
                                                </div>
                                                <!-- End id="product" -->
                                                
                                                <!-- Description tab -->
                                                <div class="tab-pane" id="description">
